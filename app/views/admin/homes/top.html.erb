<div class="col-lg-10 col-md-11 mx-auto mt-5">
  <div class="card">
    <div class="m-5">
      <h3 class="d-inline">会員一覧画面</h3>
      <table class="table mt-3">
        <thead>
          <tr>
            <th>氏名</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @end_users.each do |end_user| %>
            <tr>
              <td>
                <%= link_to admin_end_user_path(end_user) do %>
                  <%= end_user.full_name %>
                <% end %>
              </td>
              <td><%= link_to "編集", edit_admin_end_user_path(end_user), class: "btn btn-secondary" %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>